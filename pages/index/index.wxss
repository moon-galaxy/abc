.icon-sousuo:before {
  content: '\e621';
}
.index_swiper swiper {
  width: 750rpx;
  height: 340rpx;
}
.index_swiper swiper image {
  width: 100%;
}
.index_cate {
  display: flex;
  padding: 20rpx;
}
.index_cate navigator {
  flex: 1;
}
.index_cate navigator image {
  width: 100%;
}
.index_floor .floor_title image {
  width: 100vw;
}
.floor_list navigator {
  float: left;
  width: 33.33vw;
}
.floor_list navigator:nth-last-child(-n + 4) {
  height: 27.72711207vw;
  border-left: 10rpx solid #fff;
}
.floor_list navigator:nth-child(2),
.floor_list navigator:nth-child(3) {
  border-bottom: 10rpx solid #fff;
}
.floor_list navigator image {
  width: 100%;
  height: 100%;
  margin-bottom: -4px;
}
.clearfix::after {
  content: '.';
  visibility: hidden;
  display: block;
  height: 0px;
  clear: both;
}
